{
		"name": "phpfui/instadoc",
		"description": "Instant and always up to date documentation for PHP projects",
		"minimum-stability": "stable",
		"keywords": ["documentation", "phpdoc", "block", "doc", "docblock", "foundation"],
		"homepage": "https://github.com/phpfui/InstaDoc",
		"license": "MIT",
		"authors": [
				{
						"name": "Bruce Wells",
						"email": "brucekwells@gmail.com"
				}
		],
		"require": {
				"php": ">=7.1",
				"phpfui/phpfui": "^6.0",
				"scrivo/highlight.php": ">=9.17",
				"phpdocumentor/reflection-docblock": ">=3.0",
				"gitonomy/gitlib": "^1.2",
				"cebe/markdown": "^1.2"
			},
		"autoload": {
				"psr-0": {"PHPFUI\\InstaDoc\\": "src/"}
		},
		"require-dev": {
				"phpunit/phpunit": ">=7.0",
				"phpfui/html-unit-tester": "^1.0",
				"roave/security-advisories": "dev-master",
				"phpfui/phpunit-syntax-coverage": "^1.0"
		}
}
